<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="person1" class="com.abhishyam.learn.innerbean.Person">
        <property name="name" value="abhishyam"/>
        <property name="address" value="Kanigiri"/>
        <property name="age" value="25"/>
    </bean>

    <bean id="customer" class="com.abhishyam.learn.innerbean.Customer">
        <property name="person" ref="person1"/>
    </bean>
</beans>